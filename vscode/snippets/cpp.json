			"{",
			"	std::cout << \"===============================execise920===============================\" << std::endl;",
			"",
			"	std::cout << \"===============================execise920===============================\" << std::endl;",
			"}",
		],
		"description": "c/c++ execise"
	},
	"cpp_test": {
		"prefix": "cpp_test",
		"body": [
			"std::cout << \"**********************Test_ **********************\" << std::endl;",
			"",
